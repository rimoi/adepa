{% extends 'base_admin.html.twig' %}

{% block title %} Agenda {{ app.user.nickname }}{% endblock %}


{% block stylesheets %}
    {{ encore_entry_link_tags('calendar') }}
{% endblock %}

{% block body %}
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="{{ path('home') }}">Accueil</a></li>
        <li class="breadcrumb-item"><a href="{{ path('admin_dash_board_index') }}">Dashboard</a></li>
        <li class="breadcrumb-item active">{{ block('title') }}</li>
    </ol>
    <div class="cal-wrapper">
        <div id="calendar" data-info='{{ missions|raw }}'>
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('calendar') }}
{% endblock %}